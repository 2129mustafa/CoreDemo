
<div class="tech-btm">
    <p>Teknoloji, yazılım ve diğer bloglar... </p>
    <form action="/NewsLatter/SubscribeMail/" method="post">
        <input type="email" placeholder="Email" id="txtid" name="Mail" required="">
        <input type="submit" id="btn" value="Abone Ol">
    </form>
</div>

<script type="text/javascript">
    //const { url } = require("inspector");
    //const { type } = require("node:os");
    $("#btn").click(function () {
        let values = {
            Mail: $("#txtid").val()
        };
        $.ajax({
            type: "post",
            url: "/NewsLatter/SubscribeMail/",
            data: values,
            success: function (func) {
                let result = jQuery.parseJSON(func);
            }
        });
    });
</script>

